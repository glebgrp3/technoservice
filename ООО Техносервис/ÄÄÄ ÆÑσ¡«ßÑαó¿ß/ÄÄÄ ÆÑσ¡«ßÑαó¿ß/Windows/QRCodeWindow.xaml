<Window x:Class="ООО_Техносервис.Windows.QRCodeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ООО_Техносервис.Windows"
        mc:Ignorable="d"
        Title="QR код обратной связи" Height="450" Width="400">
    <Grid>
        <Image x:Name="qrCodeImageView"></Image>
        <TextBlock FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="4">Отсканируйте QR с помощью телефона</TextBlock>
    </Grid>
</Window>
