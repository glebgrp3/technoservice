<Window x:Class="ООО_Техносервис.Windows.AddClientWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ООО_Техносервис.Windows"
        mc:Ignorable="d"
        Title="Добавления нового пользователя" Height="300" Width="400" ResizeMode="NoResize">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock>Логин</TextBlock>
            <TextBox x:Name="LoginText"></TextBox>
            <TextBlock x:Name="NameText">Имя</TextBlock>
            <TextBox></TextBox>
            <TextBlock x:Name="SurNameText">Фамилия</TextBlock>
            <TextBox></TextBox>
            <TextBlock>Пароль</TextBlock>
            <PasswordBox x:Name="PasswordText"></PasswordBox>
            <Button Click="AddClientButton_Click">Добавить клиента</Button>
            <Label></Label>
            <Button x:Name="back" Click="back_Click">Назад</Button>
        </StackPanel>
    </Grid>
</Window>
